{
  "name": "build-elevate",
  "version": "1.0.7",
  "description": "Production-grade full-stack starter with Next.js, Express, and Better Auth - Get from idea to production faster",
  "type": "module",
  "author": "Vijay Singh",
  "license": "MIT",
  "homepage": "https://github.com/vijaysingh2219/build-elevate#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vijaysingh2219/build-elevate.git"
  },
  "bugs": {
    "url": "https://github.com/vijaysingh2219/build-elevate/issues"
  },
  "keywords": [
    "starter",
    "boilerplate",
    "nextjs",
    "express",
    "monorepo",
    "turborepo",
    "typescript",
    "prisma",
    "better-auth",
    "shadcn-ui",
    "fullstack",
    "template",
    "scaffold",
    "cli"
  ],
  "bin": {
    "build-elevate": "./dist/index.js"
  },
  "files": [
    "dist/index.js"
  ],
  "scripts": {
    "build": "turbo build",
    "build:cli": "tsup",
    "prepublish": "pnpm build:cli",
    "clean": "turbo clean",
    "dev": "turbo dev",
    "docker:prod": "docker-compose -f docker-compose.prod.yml up --build",
    "lint": "turbo lint",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "check-types": "turbo check-types",
    "start": "turbo start",
    "test": "turbo test",
    "prepare": "husky"
  },
  "dependencies": {
    "@clack/prompts": "catalog:cli",
    "commander": "catalog:cli",
    "degit": "catalog:cli",
    "yaml": "catalog:cli"
  },
  "devDependencies": {
    "@types/node": "catalog:dev",
    "@types/degit": "catalog:cli",
    "@workspace/typescript-config": "workspace:*",
    "eslint": "catalog:eslint",
    "husky": "catalog:dev",
    "lint-staged": "catalog:dev",
    "prettier": "catalog:prettier",
    "prettier-plugin-organize-imports": "catalog:prettier",
    "prettier-plugin-packagejson": "catalog:prettier",
    "prettier-plugin-tailwindcss": "catalog:prettier",
    "tsup": "catalog:cli",
    "turbo": "catalog:core",
    "typescript": "catalog:dev"
  },
  "packageManager": "pnpm@10.4.1",
  "engines": {
    "node": ">=20"
  }
}
